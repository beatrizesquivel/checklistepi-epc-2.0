      "source": [
        "from datetime import datetime  # Importa a classe datetime para manipular datas e horas no programa\n",
        "from zoneinfo import ZoneInfo  # Importa ZoneInfo para definir fuso horﾃ｡rio correto (ex: Brasil)\n",
        "\n",
        "# Classe que representa uma atividade com seus EPIs e EPCs\n",
        "class Atividade:\n",
        "    def __init__(self, nome, epis, epcs):\n",
        "        self.nome = nome    # Atributo que guarda o nome da atividade (ex: Pedreiro)\n",
        "        self.epis = epis    # Lista de EPIs necessﾃ｡rios para essa atividade\n",
        "        self.epcs = epcs    # Lista de EPCs necessﾃ｡rios para essa atividade\n",
        "\n",
        "    # Mﾃｩtodo para exibir os EPIs da atividade na tela\n",
        "    def exibir_epis(self):\n",
        "        print(\"\\n--- EPIs ---\")\n",
        "        for epi in self.epis:  # Percorre a lista de EPIs e imprime cada um\n",
        "            print(f\"- {epi}\")\n",
        "\n",
        "    # Mﾃｩtodo para exibir os EPCs da atividade na tela\n",
        "    def exibir_epcs(self):\n",
        "        print(\"\\n--- EPCs ---\")\n",
        "        for epc in self.epcs:  # Percorre a lista de EPCs e imprime cada um\n",
        "            print(f\"- {epc}\")\n",
        "\n",
        "# Classe que representa uma etapa da obra com as atividades possﾃｭveis nela\n",
        "class EtapaObra:\n",
        "    def __init__(self, nome, atividades):\n",
        "        self.nome = nome               # Nome da etapa (ex: Fundaﾃｧﾃ｣o, Estrutura)\n",
        "        self.atividades = atividades   # Lista de nomes de atividades permitidas nessa etapa\n",
        "\n",
        "    # Mﾃｩtodo para listar as atividades disponﾃｭveis na etapa escolhida\n",
        "    def listar_atividades(self):\n",
        "        print(f\"\\nAtividades possﾃｭveis na etapa '{self.nome}':\")\n",
        "        for atividade in self.atividades:  # Percorre a lista e imprime cada atividade\n",
        "            print(f\"- {atividade}\")\n",
        "\n",
        "# Classe principal que armazena o banco de dados de atividades e etapas\n",
        "class ObraSeguranca:\n",
        "    def __init__(self):\n",
        "        self.dados_atividades = self._carregar_atividades()  # Carrega todas as atividades disponﾃｭveis\n",
        "        self.etapas = self._carregar_etapas()                # Carrega todas as etapas da obra com suas atividades\n",
        "\n",
        "    # Mﾃｩtodo privado que define as atividades disponﾃｭveis e seus EPIs e EPCs\n",
        "    def _carregar_atividades(self):\n",
        "        return {\n",
        "            \"carpinteiro\": Atividade(\"Carpinteiro\", [\"孫窶坂凾畑\", \"Capacete\", \"Cinto\", \"Botina aﾃｧo\", \"Luvas\", \"ﾃ田ulos\"], [\"Linha vida\", \"Rede\", \"Sinalizaﾃｧﾃ｣o risco\"]),\n",
        "            \"pedreiro\": Atividade(\"Pedreiro\", [\"圦\", \"Capacete\", \"Botina aﾃｧo\", \"Luvas\", \"ﾃ田ulos\"], [\"Barreiras\", \"Sinalizaﾃｧﾃ｣o\"]),\n",
        "            \"eletricista\": Atividade(\"Eletricista\", [\"孫庁\", \"Luvas isolantes\", \"Botina dielﾃｩtrica\", \"Capacete facial\", \"ﾃ田ulos\"], [\"Bloqueio\", \"Desenergizaﾃｧﾃ｣o\", \"Sinalizaﾃｧﾃ｣o ﾃ｡rea\"]),\n",
        "            \"soldador\": Atividade(\"Soldador\", [\"捉窶昨沛ｭ\", \"Mﾃ｡scara de solda\", \"Luvas raspa\", \"Botina\", \"Avental raspa\"], [\"Ventilaﾃｧﾃ｣o\", \"Sinalizaﾃｧﾃ｣o ﾃ｡rea\", \"Organizaﾃｧﾃ｣o cabos\"]),\n",
        "            \"armador\": Atividade(\"Armador\", [\"孫窶坂凾畠\", \"Capacete\", \"Luvas vaqueta\", \"Botina aﾃｧo\", \"ﾃ田ulos\"], [\"Organizaﾃｧﾃ｣o\", \"Sinalizaﾃｧﾃ｣o\", \"Quedas\"]),\n",
        "            \"encanador\": Atividade(\"Encanador\", [\"孫窶坂凾堊\", \"Capacete\", \"Luvas borracha\", \"Botina impermeﾃ｡vel\", \"Mﾃ｡scara descartﾃ｡vel\", \"ﾃ田ulos\"], [\"Ventilaﾃｧﾃ｣o\", \"Sinalizaﾃｧﾃ｣o\", \"Organizaﾃｧﾃ｣o material\"]),\n",
        "            \"pintor\": Atividade(\"Pintor\", [\"耳\", \"Mﾃ｡scara\", \"Luvas PVC\", \"ﾃ田ulos\", \"Botina\", \"Macacﾃ｣o\"], [\"Ventilaﾃｧﾃ｣o\", \"Isolamento\", \"Sinalizaﾃｧﾃ｣o 'Piso Molhado'\"]),\n",
        "            \"mestre de obras\": Atividade(\"Mestre de Obras\", [\"孫窶坂凾搭\", \"Capacete\", \"Botina\", \"ﾃ田ulos\", \"Colete refletivo\"], [\"DDS\", \"Plano Rigging\", \"Plano Emergﾃｪncia\"]),\n",
        "            \"servente\": Atividade(\"Servente\", [\"ｧｹ\", \"Capacete\", \"Botina aﾃｧo\", \"Luvas\", \"ﾃ田ulos\"], [\"Caminhos sinalizados\", \"Descarte resﾃｭduos\", \"Organizaﾃｧﾃ｣o\"]),\n",
        "            \"supervisor de seguranﾃｧa\": Atividade(\"Supervisor de Seguranﾃｧa\", [\"圷\", \"Capacete\", \"Colete refletivo\", \"Botina\", \"ﾃ田ulos\"], [\"APR\", \"Procedimentos emergﾃｪncia\", \"Treinamentos\"]),\n",
        "        }\n",
        "\n",
        "    # Mﾃｩtodo privado que define as etapas da obra e as atividades permitidas em cada uma\n",
        "    def _carregar_etapas(self):\n",
        "        return {\n",
        "            \"fundaﾃｧﾃ｣o\": EtapaObra(\"Fundaﾃｧﾃ｣o\", [\"pedreiro\", \"armador\", \"servente\"]),\n",
        "            \"estrutura\": EtapaObra(\"Estrutura\", [\"carpinteiro\", \"armador\", \"soldador\", \"servente\"]),\n",
        "            \"instalaﾃｧﾃｵes\": EtapaObra(\"Instalaﾃｧﾃｵes\", [\"eletricista\", \"encanador\", \"supervisor de seguranﾃｧa\"]),\n",
        "            \"acabamento\": EtapaObra(\"Acabamento\", [\"pintor\", \"pedreiro\", \"servente\"]),\n",
        "            \"cobertura\": EtapaObra(\"Cobertura\", [\"carpinteiro\", \"soldador\", \"mestre de obras\"])\n",
        "        }\n",
        "\n",
        "    # Mﾃｩtodo para buscar uma atividade pelo nome digitado pelo usuﾃ｡rio\n",
        "    def buscar_atividade(self, nome_atividade):\n",
        "        nome_atividade = nome_atividade.lower().strip()  # Converte para minﾃｺsculo e tira espaﾃｧos extras\n",
        "        return self.dados_atividades.get(nome_atividade, None)  # Retorna o objeto Atividade ou None\n",
        "\n",
        "    # Mﾃｩtodo para buscar uma etapa pelo nome digitado pelo usuﾃ｡rio\n",
        "    def buscar_etapa(self, nome_etapa):\n",
        "        nome_etapa = nome_etapa.lower().strip()  # Converte para minﾃｺsculo e tira espaﾃｧos extras\n",
        "        return self.etapas.get(nome_etapa, None)  # Retorna o objeto EtapaObra ou None\n",
        "\n",
        "# Funﾃｧﾃ｣o para salvar no arquivo de histﾃｳrico o checklist realizado pelo usuﾃ｡rio\n",
        "def salvar_historico(nome_usuario, etapa, atividade):\n",
        "    agora = datetime.now(ZoneInfo(\"America/Bahia\"))  # Obtﾃｩm data/hora atual com fuso da Bahia\n",
        "    data_hora = agora.strftime(\"%d/%m/%Y %H:%M:%S\")  # Formata data/hora em dia/mﾃｪs/ano hora:minuto:segundo\n",
        "\n",
        "    # Abre (ou cria) o arquivo histﾃｳrico em modo de adicionar (append)\n",
        "    with open(\"historico_seguranca.txt\", \"a\", encoding=\"utf-8\") as arquivo:\n",
        "        arquivo.write(f\"\\n===== {data_hora} =====\\n\")\n",
        "        arquivo.write(f\"Usuﾃ｡rio: {nome_usuario}\\n\")\n",
        "        arquivo.write(f\"Etapa: {etapa.nome}\\n\")\n",
        "        arquivo.write(f\"Atividade: {atividade.nome}\\n\")\n",
        "        arquivo.write(\"--- EPIs ---\\n\")\n",
        "        for epi in atividade.epis:\n",
        "            arquivo.write(f\"- {epi}\\n\")\n",
        "        arquivo.write(\"--- EPCs ---\\n\")\n",
        "        for epc in atividade.epcs:\n",
        "            arquivo.write(f\"- {epc}\\n\")\n",
        "        arquivo.write(\"=========================\\n\")\n",
        "\n",
        "# Funﾃｧﾃ｣o principal do programa\n",
        "def main():\n",
        "    print(\"=== CHECKLIST EPIs e EPCs ===\")\n",
        "\n",
        "    obra = ObraSeguranca()  # Cria o banco de dados de seguranﾃｧa da obra\n",
        "    nome_usuario = input(\"Digite seu nome antes de iniciar o checklist: \").strip()\n",
        "\n",
        "    # Loop atﾃｩ o usuﾃ｡rio digitar uma etapa vﾃ｡lida\n",
        "    while True:\n",
        "        etapa_input = input(\"Em qual etapa da obra vocﾃｪ estﾃ｡? (Ex: fundaﾃｧﾃ｣o, estrutura, instalaﾃｧﾃｵes, acabamento, cobertura): \")\n",
        "        etapa = obra.buscar_etapa(etapa_input)\n",
        "\n",
        "        if etapa:\n",
        "            break  # Sai do loop se a etapa for encontrada\n",
        "        else:\n",
        "            print(f\"Etapa '{etapa_input}' nﾃ｣o encontrada. Tente novamente.\\n\")\n",
        "\n",
        "    # Loop atﾃｩ o usuﾃ｡rio digitar uma atividade vﾃ｡lida dentro da etapa escolhida\n",
        "    while True:\n",
        "        etapa.listar_atividades()  # Lista atividades disponﾃｭveis na etapa\n",
        "        atividade_input = input(\"Digite a atividade a ser exercida nesta etapa: \").lower().strip()\n",
        "\n",
        "        if atividade_input in etapa.atividades:\n",
        "            atividade = obra.buscar_atividade(atividade_input)\n",
        "            if atividade:\n",
        "                atividade.exibir_epis()   # Exibe EPIs necessﾃ｡rios na tela\n",
        "                atividade.exibir_epcs()   # Exibe EPCs necessﾃ｡rios na tela\n",
        "                salvar_historico(nome_usuario, etapa, atividade)  # Salva tudo no histﾃｳrico.txt\n",
        "                break  # Sai do loop apﾃｳs concluir\n",
        "            else:\n",
        "                print(\"Atividade nﾃ｣o encontrada no banco de dados. Tente novamente.\\n\")\n",
        "        else:\n",
        "            print(f\"Atividade '{atividade_input}' nﾃ｣o pertence ﾃ etapa '{etapa.nome}'. Tente novamente.\\n\")\n",
        "\n",
        "    print(\"\\nSEGURANﾃ② EM PRIMEIRO LUGAR 笵曾")\n",
        "\n",
        "# Executa o programa principal se for o arquivo principal\n",
        "if __name__ == \"__main__\":\n",
        "    main()\n"
      ],
